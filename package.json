{
  "name": "frakture-react-reports",
  "version": "0.9.9",
  "description": "Frakture React Reporting components -- library to include frakture reporting in your react report",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "@date-io/dayjs": "^1.3.13",
    "dayjs": "^1.8.23",
    "query-string": "^6.11.1",
    "react-grid-layout": "^0.18.3",
    "react-hook-form": "^5.1.3",
    "recharts": "^1.8.5"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0",
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/pickers": "^3.2.10"
  },
  "devDependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/pickers": "^3.2.10",
    "nwb": "0.24.x",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0"
  },
  "author": "Frakture, Inc",
  "homepage": "https://www.frakture.com",
  "license": "MIT",
